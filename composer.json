{
    "name": "squadms/discord",
    "description": "The SquadMS Discord integration module.",
    "keywords": ["laravel", "squad", "squadms", "discord"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Patrick Henninger",
            "email": "privat@skyraptor.eu",
            "homepage": "https://skyraptor.eu/"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "nesbot/carbon": "^2.0",
        "restcord/restcord": "v9.x-dev",
        "squadms/foundation": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "SquadMS\\Discord\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SquadMS\\Discord\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SquadMS\\Discord\\DiscordServiceProvider",
                "SquadMS\\Discord\\Providers\\EloquentServiceProvider",
                "SquadMS\\Discord\\Providers\\FilamentServiceProvider",
                "SquadMS\\Discord\\Providers\\ModulesServiceProvider",
                "SquadMS\\Discord\\Providers\\PermissionsServiceProvider"
            ]
        }
    }
}
